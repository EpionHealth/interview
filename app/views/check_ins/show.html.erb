<p>Please complete all of the steps on this page:</p>
<ul>
  <% Screener.all.each do |screener| %>
    <li><%= button_to "Start #{screener.name} screener", check_in_screener_path(@check_in, screener), method: :get %></li>
  <% end %>
</ul>

<%= form_for(:check_in, url: check_in_path, method: :put, id: 1) do |f| %>
  <%= f.submit "Complete check in" %>
<% end %>
